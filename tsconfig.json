{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "importHelpers": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "paths": {
      "@/*": ["src/*"],
      "@@/*": [".dumi/tmp/*"]
    }
  },
  "include": [".dumirc.ts", "typings.d.ts"],
  "exclude": [
    "node_modules",
    "lib",
    "es",
    "typings",
    "**/__test__",
    "test",
    "tests",
    "docs-3d"
  ]
}
